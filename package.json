{
  "name": "sugarsugar",
  "version": "1.0.0",
  "description": "webservice to ask dexcom for blood glucose data",
  "main": "api/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "start": "node api/index.js",
    "dev": "node --watch api/index.js",
    "build": "node build.js",
    "vercel-build": "npm run build",
    "test": "node test.js",
    "setup": "cp .env.vercel.example .env.vercel && echo 'Please edit .env.vercel with your actual credentials'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky"
  },
  "keywords": [
    "dexcom",
    "glucose",
    "api",
    "cgm",
    "diabetes"
  ],
  "author": "Dave Alger <me@davealger.com>",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.6.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2"
  },
  "lint-staged": {
    "*.{js,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  }
}
